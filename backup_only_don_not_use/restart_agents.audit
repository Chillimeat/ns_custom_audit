<check_type:"Unix">
#restart或stop nessusagent无效
<custom_item>
	system			:	"Linux"
	type			:	CMD_EXEC
	description		:	"Restart Agent"
	cmd				:	"find / -not -path \"/proc/*\" -type f -name assetemp.log -exec ls  -lh {} \\;; service nessusagent stop && service nessusagent start && service nessusagent status"
	expect		:	""
#	severity    	: 	MEDIUM
#	quiet			:	YES
	dont_echo_cmd	:	YES
</custom_item>

</check_type>
